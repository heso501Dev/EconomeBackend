{
  "name": "surrealdb.node",
  "version": "0.3.0",
  "author": {
    "name": "Raphael Darley",
    "email": "raphael.darley@surrealdb.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/surrealdb/surrealdb.node"
  },
  "description": "Node.js driver for SurrealDB",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "surrealdb.node",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.3",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "node move_artifacts.js",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@3.6.3",
  "optionalDependencies": {
    "surrealdb.node-win32-x64-msvc": "0.3.0",
    "surrealdb.node-darwin-x64": "0.3.0",
    "surrealdb.node-linux-x64-gnu": "0.3.0",
    "surrealdb.node-darwin-arm64": "0.3.0",
    "surrealdb.node-android-arm64": "0.3.0",
    "surrealdb.node-linux-arm64-gnu": "0.3.0",
    "surrealdb.node-linux-arm64-musl": "0.3.0",
    "surrealdb.node-win32-arm64-msvc": "0.3.0",
    "surrealdb.node-linux-arm-gnueabihf": "0.3.0",
    "surrealdb.node-linux-x64-musl": "0.3.0",
    "surrealdb.node-freebsd-x64": "0.3.0",
    "surrealdb.node-win32-ia32-msvc": "0.3.0",
    "surrealdb.node-android-arm-eabi": "0.3.0",
    "surrealdb.node-darwin-universal": "0.3.0"
  }
}